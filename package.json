{
  "name": "purchase-service",
  "packageManager": "npm@10.2.4",
  "main": "index.js",
  "dependencies": {
    "@azure/service-bus": "^7.9.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "mssql": "^12.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^7.0.0-alpha.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/mssql": "^9.1.8",
    "@types/node": "^25.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "swagger-autogen": "^2.23.7",
    "swagger-typescript-api": "^13.2.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run tsoa:gen && ts-node src/index.ts",
    "prestart": "npm run tsoa:gen",
    "tsoa:gen": "tsoa spec-and-routes",
    "generate-types": "swagger-typescript-api generate -p ./src/generated/swagger-output.json -o ./src/generated -n api-types.ts",
    "dev": "ts-node src/index.ts"
  }
}